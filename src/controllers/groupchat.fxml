<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.GroupController">
   <children>
      <Rectangle fill="DODGERBLUE" height="400.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" width="300.0" />
      <TextField fx:id="groupName" layoutX="50.0" layoutY="51.0" prefHeight="25.0" prefWidth="200.0" />
      <Button layoutX="50.0" layoutY="353.0" mnemonicParsing="false" onAction="#goBack" prefHeight="25.0" prefWidth="72.0" style="-fx-background-insets: 0; -fx-background-color: #ffffff;" text="Go Back" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Candara Bold" size="12.0" />
         </font></Button>
      <ScrollPane hbarPolicy="NEVER" layoutX="50.0" layoutY="114.0" prefHeight="200.0" prefWidth="200.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <VBox fx:id="activeFriendBox" prefHeight="200.0" prefWidth="200.0" spacing="5.0">
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="20.0" top="5.0" />
                     </padding></VBox>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
      <Text fill="#fffdfd" layoutX="50.0" layoutY="35.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter Group Name :" />
      <Text fill="WHITE" layoutX="50.0" layoutY="100.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Group Members :" />
      <Button fx:id="startChat" layoutX="177.0" layoutY="353.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="72.0" style="-fx-background-insets: 0; -fx-background-color: #ffffff;" text="Start Chat" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Candara Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
